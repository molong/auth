{
    "name": "tensent/think-auth",
    "description": "thinkphp auth",
    "type": "think-extend",
    "license": "MIT",
    "authors": [
        {
            "name": "molong",
            "email": "ycgpp@126.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "tensent\\auth\\": "src/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "tensent\\auth\\AuthServiceProvider"
            ],
            "config": {
                "auth": "config/auth.php"
            }
        }
    }
}
